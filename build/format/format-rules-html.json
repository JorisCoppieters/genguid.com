{
    "allow_empty_files": true,
    "block_elements": [
        "tg-icon"
    ],
    "definition_type": "HTML",
    "format_multi_classes_with_at_least": 3,
    "inline_elements": [],
    "line_ending": "\n",
    "convert_line_endings": true,
    "allow_arbitrary_closing_html_tags": true,
    "remove_css": false,
    "multi_classes_order": [
        "^o-.*$",
        "^h-.*$",
        "^tm-.*$",
        "^mp-.*$",
        "^jb-.*$",
        "^pt-.*$",
        "^mt-.*$",
        ".*"
    ],
    "ng_version": 2,
    "ng_attributes_order": [
        "sm.*",
        "sd.*",
        "md.*",
        "mg.*",
        "lg.*",
        "ll.*",
        "xl.*"
    ],
    "ng_attributes_order_pre_native": [
        "\\*.*",
        "\\#.*",
        "\\[\\(.*\\)\\]",
        "\\(.*\\)",
        "\\[sm.*\\]",
        "\\[sd.*\\]",
        "\\[md.*\\]",
        "\\[mg.*\\]",
        "\\[lg.*\\]",
        "\\[ll.*\\]",
        "\\[xl.*\\]",
        "\\[.*\\]"
    ]
}
