    "scripts": {
        "check:all": "yarn check:build && yarn check:unused-exports && yarn check:modules && yarn check:dependencies && yarn check:ts && yarn check:scss",
        "check:all:ci": "yarn check:build && yarn check:unused-exports && yarn check:ts && yarn check:scss",
        "check:build": "tsc --build ./tsconfig.check.json",
        "check:dependencies": "depcheck --ignore-patterns=*.scss",
        "check:modules": "npm-check --ignore socket.io --ignore typescript --ignore zone.js --ignore karma --ignore sqlite3",
        "check:ts": "bash ./tools/checks/check-ts.sh",
        "check:scss": "bash ./tools/checks/check-scss.sh",
        "check:site:prod": "protractor ./src/tests/check/prod-site/protractor.conf.js",
        "check:site:test": "protractor ./src/tests/check/test-site/protractor.conf.js",
        "check:unused-exports": "ts-unused-exports ./tsconfig.check.json",
        "check:update": "yarn check:modules && yarn check:dependencies",
        "coverage": "nyc report --reporter lcovonly -t coverage && nyc report --reporter html -t coverage && codecov --disable=gcov && nyc report --reporter text-summary -t coverage && nyc check-coverage -t coverage",
        "coverage:dev": "yarn test:core && yarn test:client && yarn coverage:html && yarn coverage:html:open",
        "coverage:html": "nyc report --reporter lcovonly -t coverage && nyc report --reporter html -t coverage",
        "coverage:html:open": "chrome ./coverage/index.html",
        "deploy": "bash ./tools/scripts/deploy.sh",
        "format": "node ./tools/format/format.js",
        "lint": "eslint -c .eslintrc.js --ext .ts ./src",
        "new:component": "cd src/client/app/ && ng generate component",
        "precommit": "yarn lint --fix && yarn format --fix && yarn check:all",
        "precoverage": "istanbul-merge --out ./coverage/coverage-final.json ./.coverage/core/coverage-final.json ./.coverage/site/coverage-final.json && nyc report -t coverage --reporter html",
        "precoverage:html": "istanbul-merge --out ./coverage/coverage-final.json ./.coverage/core/coverage-final.json ./.coverage/site/coverage-final.json && nyc report -t coverage --reporter html",
        "predeploy": "yarn update:webdriver && yarn test && yarn coverage",
        "preserve": "yarn lint --fix && yarn format --fix",
        "pretest:e2e:ci": "webdriver-manager update -- --gecko=false",
        "rollback": "bash ./tools/scripts/rollback.sh",
        "serve": "bash ./tools/scripts/serve.sh",
        "test": "yarn test:e2e:quick && yarn test:client && yarn test:core",
        "test:client": "ng test --browsers=ChromeHeadless --watch=false",
        "test:client:watch": "ng test --browsers=ChromeHeadless",
        "test:core": "jest --silent",
        "test:core:verbose": "jest",
        "test:e2e": "bash ./tools/scripts/e2e-tests.sh",
        "test:e2e:ci": "bash ./tools/scripts/e2e-tests.sh ci",
        "test:e2e:quick": "bash ./tools/scripts/e2e-tests.sh",
        "update": "ncu -u && yarn install --check-files",
        "update:webdriver": "webdriver-manager update"
    },
    "version": "1.0.0"
}
